<Defs>
	<!-- ==========[Base]========== -->
	<ThingDef ParentName="EggFertBase" Abstract="True" Name="Mashed_Spiribird_EggBase">
		<description>A fertilized spiribird egg. If all goes well, it should hatch into a chick. It can be eaten raw, but it's much better cooked.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Mashed_SpiribirdEggs</texPath>
		</graphicData>
		<statBases>
			<MarketValue>7</MarketValue>
		</statBases>
	</ThingDef>
