<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName = "GR_ExtractGenesBird"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>Corpse_Mashed_Spiribird_SpiribirdGreen</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdOrange</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdPink</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdRed</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdYellow</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdPrism</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName = "GR_ExtractGenesBird"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>Corpse_Mashed_Spiribird_SpiribirdGreen</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdOrange</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdPink</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdRed</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdYellow</li>
						<li>Corpse_Mashed_Spiribird_SpiribirdPrism</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<GeneticRim.ExtractableAnimalsList>
							<defName>GR_TierOneAnimalsBird_Mashed_Spiribirds</defName>
							<tier>1</tier>
							<itemProduced>GR_AvianGenetic</itemProduced>
							<extractableAnimals>
								<li>Mashed_Spiribird_SpiribirdGreen</li>
								<li>Mashed_Spiribird_SpiribirdOrange</li>
								<li>Mashed_Spiribird_SpiribirdPink</li>
								<li>Mashed_Spiribird_SpiribirdRed</li>
								<li>Mashed_Spiribird_SpiribirdYellow</li>
								<li>Mashed_Spiribird_SpiribirdPrism</li>
							</extractableAnimals>
						</GeneticRim.ExtractableAnimalsList>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
